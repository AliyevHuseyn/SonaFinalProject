@model List<FAQ>


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Treker - FAQ</title>
    <!-- Stylesheets -->
    <link href="~/additional/css/bootstrap.css" rel="stylesheet">
    <link href="~/additional/css/style.css" rel="stylesheet">
    <link rel="shortcut icon" href="images/favicon.png" type="image/x-icon">
    <link rel="icon" href="images/favicon.png" type="image/x-icon">
    <!-- Responsive -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link href="~/additional/css/responsive.css" rel="stylesheet">
    <link rel="stylesheet" href="~/assets/css/font-awesome.min.css" type="text/css">
    <!--[if lt IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.js"></script><![endif]-->

</head>

<body>

    <!-- Breadcrumb Section Begin -->
    <div class="breadcrumb-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb-text">
                        <h2>FAQ</h2>
                        <div class="bt-option">
                            <a asp-action="index" asp-controller="home">Home</a>
                            <span>FAQ</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Breadcrumb Section End -->
    <!--FAQs Section-->
    <section class="faq-one alternate">
        <div class="auto-container">
            <div class="title-box centered">
                <h2><span>Frequently Asked Question</span></h2>
            </div>
            <div class="row clearfix">
                <!--Text Col-->
                <div class="text-col col-lg-12 col-md-12 col-sm-12">
                    <div class="inner wow fadeInLeft" data-wow-duration="1500ms" data-wow-delay="0ms">
                        <div class="accordion-box clearfix">
                            <!--Block-->
                            @foreach (var faq in Model)
                            {
                                <div class="accordion block">
                                    <div class="acc-btn">@faq.Title <i class="fa-regular fa-angle-down"></i></div>
                                    <div class="acc-content">
                                        <div class="content">
                                            <div class="text">@faq.Description</div>
                                        </div>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                </div>
                <!--Text Col-->
            </div>
        </div>
    </section>
    <script src="~/additional/js/jquery.js"></script>
    <script src="~/additional/js/popper.min.js"></script>
    <script src="~/additional/js/bootstrap.min.js"></script>
    <script src="~/additional/js/jquery-ui.js"></script>
    <script src="~/additional/js/jquery.fancybox.js"></script>
    <script src="~/additional/js/mixitup.js"></script>
    <script src="~/additional/js/touchspin.js"></script>
    <script src="~/additional/js/owl.js"></script>
    <script src="~/additional/js/wow.js"></script>
    <script src="~/additional/js/custom-script.js"></script>
</body>
</html>





